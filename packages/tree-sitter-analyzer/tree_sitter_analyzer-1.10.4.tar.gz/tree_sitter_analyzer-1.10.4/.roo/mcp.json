{
    "mcpServers": {
        "tree-sitter-analyzer-local": {
            "command": "uv",
            "args": [
                "run",
                "--directory",
                "C:/git-private/tree-sitter-analyzer",
                "python",
                "-m",
                "tree_sitter_analyzer.mcp.server"
            ],
            "env": {
                "TREE_SITTER_PROJECT_ROOT": "C:/git-private/tree-sitter-analyzer",
                "TREE_SITTER_OUTPUT_PATH": "C:/git-public/tree-sitter-analyzer/.analysis",
                "TREE_SITTER_ANALYZER_ENABLE_FILE_LOG": "true",
                "TREE_SITTER_ANALYZER_LOG_DIR": "C:/git-private/tree-sitter-analyzer/.analysis",
                "TREE_SITTER_ANALYZER_LOG_LEVEL": "DEBUG"
            },
            "alwaysAllow": [
                "check_code_scale",
                "extract_code_section",
                "set_project_path",
                "query_code",
                "analyze_code_structure",
                "list_files",
                "find_and_grep",
                "search_content"
            ],
            "timeout": 3600,
            "disabled": false
        },
        "tree-sitter-analyzer-bc-test": {
            "command": "uv",
            "args": [
                "run",
                "--directory",
                "C:/git-private/tree-sitter-analyzer",
                "python",
                "-m",
                "tree_sitter_analyzer.mcp.server"
            ],
            "env": {
                "TREE_SITTER_PROJECT_ROOT": "C:/git-private/tree-sitter-analyzer",
                "TREE_SITTER_OUTPUT_PATH": "C:/git-private/tree-sitter-analyzer/.analysis-bc-test"
            },
            "alwaysAllow": [
                "check_code_scale",
                "extract_code_section",
                "set_project_path",
                "query_code",
                "list_files",
                "find_and_grep",
                "search_content"
            ],
            "timeout": 3600,
            "disabled": true
        },
        "tree-sitter-analyzer-1.6.1.2": {
            "command": "uv",
            "args": [
                "run",
                "--with",
                "tree-sitter-analyzer[mcp]==1.6.1.2",
                "python",
                "-m",
                "tree_sitter_analyzer.mcp.server"
            ],
            "env": {
                "TREE_SITTER_PROJECT_ROOT": "C:/git-private/tree-sitter-analyzer",
                "TREE_SITTER_OUTPUT_PATH": "C:/git-private/tree-sitter-analyzer/.analysis-1.6.1.2"
            },
            "alwaysAllow": [
                "analyze_code_structure",
                "query_code",
                "check_code_scale",
                "extract_code_section",
                "set_project_path",
                "list_files",
                "search_content"
            ],
            "timeout": 3600,
            "disabled": true
        },
        "tree-sitter-analyzer-1.9.2": {
            "command": "uv",
            "args": [
                "run",
                "--with",
                "tree-sitter-analyzer[mcp]==1.9.2",
                "python",
                "-m",
                "tree_sitter_analyzer.mcp.server"
            ],
            "env": {
                "TREE_SITTER_PROJECT_ROOT": "C:/git-private/tree-sitter-analyzer",
                "TREE_SITTER_OUTPUT_PATH": "C:/git-private/tree-sitter-analyzer/.analysis-1.9.2"
            },
            "alwaysAllow": [
                "analyze_code_structure",
                "query_code",
                "check_code_scale",
                "set_project_path",
                "list_files",
                "find_and_grep",
                "search_content"
            ],
            "timeout": 3600,
            "disabled": true
        },
        "multi-agent-dialogue": {
            "command": "uv",
            "args": [
                "run",
                "python",
                "-m",
                "multi_agent_dialogue.mcp_server"
            ],
            "env": {
                "MAD_SERVER_URL": "ws://localhost:8765/ws",
                "MAD_AGENT_TYPE": "roo-code",
                "MAD_AGENT_ID": "roo-agent-2",
                "MAD_MODEL_NAME": "claude-4-sonnet",
                "MAD_CAPABILITIES": "code,chat,review"
            },
            "autoConnect": true,
            "disabled": true,
            "alwaysAllow": [
                "send_message",
                "join_session",
                "get_messages"
            ]
        }
    }
}
