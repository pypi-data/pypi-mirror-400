{
  "name": "celq",
  "version": "0.1.2-alpha.1",
  "bin": "lib/index.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "dev": "yarn build && node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IvanIsCoding/celq"
  },
  "author": "Ivan Carvalho <ivancarvalho@gatech.edu>",
  "bugs": {
    "url": "https://github.com/IvanIsCoding/celq/issues"
  },
  "homepage": "https://github.com/IvanIsCoding/celq#readme",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@ivaniscoding/celq-darwin-arm64": "0.1.2-alpha.1",
    "@ivaniscoding/celq-darwin-x64": "0.1.2-alpha.1",
    "@ivaniscoding/celq-linux-arm64": "0.1.2-alpha.1",
    "@ivaniscoding/celq-linux-x64": "0.1.2-alpha.1",
    "@ivaniscoding/celq-windows-arm64": "0.1.2-alpha.1",
    "@ivaniscoding/celq-windows-x64": "0.1.2-alpha.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "lib/*"
    ],
    "root": true
  }
}
