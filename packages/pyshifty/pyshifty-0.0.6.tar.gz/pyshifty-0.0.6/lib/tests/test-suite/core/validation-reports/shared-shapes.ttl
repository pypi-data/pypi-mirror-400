@prefix ex: <http://example.org/shacl-test/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ex:s1 a sh:NodeShape ;
    sh:property ex:s2,
        ex:s3 ;
    sh:targetNode ex:i .

ex:s2 sh:path ex:p ;
    sh:property ex:s4 .

ex:s3 sh:path ex:q ;
    sh:property ex:s4 .

ex:s4 sh:class ex:C ;
    sh:path ex:r .

