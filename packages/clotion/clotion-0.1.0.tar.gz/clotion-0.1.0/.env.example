# Notion Integration
NOTION_API_KEY=secret_xxxxxxxxxxxxx
NOTION_DATABASE_ID=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Notion Status Values (must match your Status property options exactly)
NOTION_STATUS_BACKLOG=Backlog
NOTION_STATUS_TODO=Todo
NOTION_STATUS_IN_PROGRESS=In Progress
NOTION_STATUS_IN_REVIEW=In Review
NOTION_STATUS_DONE=Done

# Repository
REPO_PATH=/path/to/your/repo

# Polling (seconds between checks)
POLL_INTERVAL=5

# Task Settings
MAX_CONCURRENT_TASKS=5
BLOCKED_TIMEOUT=3600

# Logging
LOG_LEVEL=INFO
