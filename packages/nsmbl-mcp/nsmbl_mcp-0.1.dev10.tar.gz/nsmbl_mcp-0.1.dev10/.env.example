# NSMBL API Configuration
# Copy this file to .env and fill in your values

# Required: Your NSMBL API key (get from https://app.nsmbl.io)
NSMBL_API_KEY=your_api_key_here

# Optional: API base URL (default shown below)
NSMBL_API_BASE_URL=https://api.nsmbl.io/api/v1

# Optional: Request timeout in seconds (default: 30)
NSMBL_REQUEST_TIMEOUT=30

# Optional: Backtest polling configuration
NSMBL_BACKTEST_POLL_INTERVAL=5
NSMBL_BACKTEST_DEFAULT_TIMEOUT=300
