{
  "title": "VersionInfo",
  "type": "object",
  "additionalProperties": false,
  "required": ["service_name", "service_version", "supported_api_versions"],
  "properties": {
    "service_name": {
      "type": "string"
    },
    "service_version": {
      "type": "string"
    },
    "supported_api_versions": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1
    },
    "build": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "commit": {
          "type": "string"
        },
        "built_at": {
          "type": "string",
          "format": "date-time"
        },
        "dirty": {
          "type": "boolean"
        }
      }
    },
    "extensions": {
      "$ref": "extensions.schema.json"
    }
  }
}

