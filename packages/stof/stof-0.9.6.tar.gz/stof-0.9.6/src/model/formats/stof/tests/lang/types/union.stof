fn accepts_multiple(val: str | int | bool | ver) -> unknown {
    val
}

#[test]
fn union_types() {
    assert_eq(self.accepts_multiple('hello'), 'hello');
    assert_eq(self.accepts_multiple(456), 456);
    assert_eq(self.accepts_multiple(true), true);
    assert_eq(self.accepts_multiple(1.2.3), 1.2.3);
}
