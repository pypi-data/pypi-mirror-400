# Jaeger configuration (all settings use OTEL_MCP_ prefix)
OTEL_MCP_JAEGER_URL=http://localhost:16686
OTEL_MCP_JAEGER_TIMEOUT=30
OTEL_MCP_BACKEND_TYPE=jaeger

# Logging
OTEL_MCP_LOG_LEVEL=INFO
# OTEL_MCP_DEBUG_LOG_FILE=otel-mcp.log  # Uncomment to write debug logs to file

# Self-telemetry (optional - disable with OTEL_SDK_DISABLED=true)
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_SERVICE_NAME=otel-mcp
