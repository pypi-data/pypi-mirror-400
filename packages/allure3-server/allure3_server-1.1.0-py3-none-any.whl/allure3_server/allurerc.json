{
  "name": "Allure3-Server",
  "plugins": {
	"dashboard": {
      "options": {
      "singleFile": false,
      "reportName": "Dashboard",
      "reportLanguage": "zh",
        "layout": [
          {
            "type": "pie",
            "title": "Test Status Distribution"
          },
          {
            "type": "trend",
            "dataType": "status",
            "title": "Status Over Time"
          },
          {
            "type": "bar",
            "dataType": "statusBySeverity",
            "title": "Results by Severity"
          }
        ]
      }
    },
    "awesome": {
      "options": {
        "reportName": "Allure3",
        "singleFile": false,
        "reportLanguage": "zh",
        "open": false,
        "publish": true
      }
    },
    "allure2": {
      "options": {
        "reportName": "Allure2",
        "singleFile": false,
        "reportLanguage": "zh"
      }
    },
    "csv": {
      "options": {
        "fileName": "allure-report.csv"
      }
    },
    "log": {
      "options": {
        "groupBy": "none"
      }
    }
  },
  "variables": {
    "env_variable": "unknown"
  }
}