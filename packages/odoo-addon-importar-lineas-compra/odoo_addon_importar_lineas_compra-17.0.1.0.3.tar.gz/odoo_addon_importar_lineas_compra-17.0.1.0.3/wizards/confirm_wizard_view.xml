<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="confirm_message_wizard_view" model="ir.ui.view">
			<field name="name">purchase.order.import.confirm.wizard.form</field>
			<field name="model">purchase.order.import.confirm.wizard</field>
			<field name="arch" type="xml">
				<form string="Confirmation">
					<div class="o_form_label o_form_field_text">
						<field name="message" nolabel="1" readonly="1" widget="html"/>
					</div>
					<footer>
						<button string="OK" name="action_ok" type="object" class="btn-primary"/>
					</footer>
				</form>
			</field>
		</record>
	</data>
</odoo>
