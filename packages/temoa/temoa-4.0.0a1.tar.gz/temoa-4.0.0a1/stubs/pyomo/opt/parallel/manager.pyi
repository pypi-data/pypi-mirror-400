import enum

from _typeshed import Incomplete

class ActionStatus(str, enum.Enum):
    done = 'done'
    error = 'error'
    queued = 'queued'
    executing = 'executing'
    unknown = 'unknown'

def solve_all_instances(solver_manager, solver, instances, **kwds) -> None: ...

