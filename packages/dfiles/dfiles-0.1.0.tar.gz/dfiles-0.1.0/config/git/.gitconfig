[core]
    # Your git configuration
    # This is a template - customize as needed
    autocrlf = input
    safecrlf = warn
    editor = nano

[user]
    # TODO: Set your git user
    # name = Your Name
    # email = your.email@example.com

[init]
    defaultBranch = main

[pull]
    rebase = true

[push]
    default = current

[alias]
    # Common aliases
    st = status
    co = checkout
    br = branch
    ci = commit
    unstage = reset HEAD --
    last = log -1 HEAD
    visual = log --graph --oneline --all
