{
    "wstl": {
        "title": "HEA Server Buckets Microservice",
        "actions": [
            {
                "name": "heaserver-buckets-bucket-get-open-choices",
                "description": "Open this bucket",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Open"
            },
            {
                "name": "heaserver-buckets-bucket-get-create-choices",
                "description": "Create a new object in this bucket",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "New"
            },
            {
                "name": "heaserver-buckets-bucket-get-trash",
                "description": "Open this volume's trash",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Trash"
            },
            {
                "name": "heaserver-buckets-bucket-open-content",
                "description": "Open content",
                "type": "safe",
                "action": "read",
                "target": "app read cj",
                "prompt": "Open"
            },
            {
                "name": "heaserver-buckets-bucket-create-folder",
                "description": "Create a new folder",
                "type": "safe",
                "action": "read",
                "target": "app read cj",
                "prompt": "New Folder"
            },
            {
                "name": "heaserver-buckets-bucket-create-project",
                "description": "Create a new project",
                "type": "safe",
                "action": "read",
                "target": "app read cj",
                "prompt": "New Project"
            },
            {
                "name": "heaserver-buckets-bucket-open",
                "type": "safe",
                "target": "item",
                "prompt": "Open"
            },
            {
                "name": "heaserver-buckets-bucket-get-self",
                "description": "View this bucket",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View"
            },
            {
                "name": "heaserver-buckets-bucket-get-volume",
                "description": "View this bucket's volume",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View volume"
            },
            {
                "name": "heaserver-buckets-item-get-volume",
                "description": "View this bucket item's volume",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View volume"
            },
            {
                "name": "heaserver-buckets-bucket-get-awsaccount",
                "description": "View this bucket's AWS account",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View account"
            },
            {
                "name": "heaserver-buckets-bucket-get-upload-form",
                "description": "View get bucket's upload form",
                "type": "safe",
                "action": "read",
                "target": "item cj-template",
                "prompt": "Get target folder",
                "inputs": [
                    {
                        "name": "display_name",
                        "prompt": "Name",
                        "hea": {
                            "display": false
                        }
                    },
                    {
                        "name": "target_type",
                        "prompt": "Type",
                        "value": "heaobject.data.AWSS3FileObject",
                        "readOnly": true,
                        "hea": {
                            "display": false
                        }
                    },
                    {
                        "name": "storage_class",
                        "prompt": "Storage",
                        "type": "select",
                        "required": true,
                        "suggest": [
                            {"value":  "STANDARD", "text":  "Standard"},
                            {"value":  "DEEP_ARCHIVE", "text":  "Glacier Deep Archive"},
                            {"value":  "GLACIER", "text":  "Glacier Flexible Retrieval"},
                            {"value":  "GLACIER_IR", "text":  "Glacier Instant Retrieval"}
                        ],
                        "value": "DEEP_ARCHIVE"
                    }
                ]
            },
            {
                "name": "heaserver-buckets-bucket-get-uploader",
                "description": "Upload to Bucket",
                "type": "unsafe",
                "action": "update",
                "target": "item read cj",
                "prompt": "Upload"
            },
            {
                "name": "heaserver-buckets-bucket-get-properties",
                "description": "View and edit this Bucket's properties",
                "type": "unsafe",
                "target": "item cj-template",
                "action": "update",
                "prompt": "Properties",
                "inputs": [
                    {
                        "name": "id",
                        "prompt": "Id",
                        "readOnly": true,
                        "hea": {
                            "display": false
                        }
                    },
                    {
                        "name": "display_name",
                        "prompt": "Name",
                        "required": true,
                        "readOnly": true
                    },
                    {
                        "name": "type",
                        "readOnly": true,
                        "required": true,
                        "hea": {
                            "display": false
                        }
                    },
                    {
                        "name": "type_display_name",
                        "prompt": "Type",
                        "readOnly": true,
                        "required": true,
                        "value": "AWS S3 Bucket"
                    },
                    {
                        "name": "arn",
                        "prompt": "Amazon Resource Name",
                        "readOnly": true
                    },
                    {
                        "name": "encrypted",
                        "prompt": "Encrypted",
                        "readOnly": true,
                        "type": "select",
                        "suggest": [
                            {"value":  "true", "text":  "True"},
                            {"value":  "false", "text":  "False"}
                        ]
                    },
                    {
                        "name": "versioned",
                        "prompt": "Versioned",
                        "readOnly": true,
                        "type": "select",
                        "suggest": [
                            {"value":  "true", "text":  "True"},
                            {"value":  "false", "text":  "False"}
                        ]
                    },
                    {
                        "name": "locked",
                        "prompt": "Locked",
                        "readOnly": true,
                        "type": "select",
                        "suggest": [
                            {"value":  "true", "text":  "True"},
                            {"value":  "false", "text":  "False"}
                        ]
                    },
                    {
                        "name": "region",
                        "prompt": "Region",
                        "readOnly": true,
                        "type": "select",
                        "suggest": [
                            {"value": "us-east-2", "text": "US East (Ohio)"},
                            {"value": "us-east-1", "text": "US East (N. Virginia)"},
                            {"value": "us-west-1", "text": "US West (N. California)"},
                            {"value": "us-west-2", "text": "US West (Oregon)"},
                            {"value": "af-south-1", "text": "Africa (Cape Town)"},
                            {"value": "ap-east-1", "text": "Asia Pacific (Hong Kong)"},
                            {"value": "ap-southeast-3", "text": "Asia Pacific (Jakarta)"},
                            {"value": "ap-south-1", "text": "Asia Pacific (Mumbai)"},
                            {"value": "ap-south-2", "text": "Asia Pacific (Hyderabad)"},
                            {"value": "ap-northeast-3", "text": "Asia Pacific (Osaka)"},
                            {"value": "ap-northeast-2", "text": "Asia Pacific (Seoul)"},
                            {"value": "ap-southeast-1", "text": "Asia Pacific (Singapore)"},
                            {"value": "ap-southeast-2", "text": "Asia Pacific (Sydney)"},
                            {"value": "ap-northeast-1", "text": "Asia Pacific (Tokyo)"},
                            {"value": "ca-central-1", "text": "Canada (Central)"},
                            {"value": "cn-north-1", "text": "China (Beijing)"},
                            {"value": "cn-northwest-1", "text": "China (Ningxia)"},
                            {"value": "eu-central-1", "text": "Europe (Frankfurt)"},
                            {"value": "eu-west-1", "text": "Europe (Ireland)"},
                            {"value": "eu-west-2", "text": "Europe (London)"},
                            {"value": "eu-south-1", "text": "Europe (Milan)"},
                            {"value": "eu-south-2", "text": "Europe (Spain)"},
                            {"value": "eu-west-3", "text": "Europe (Paris)"},
                            {"value": "eu-north-1", "text": "Europe (Stockholm)"},
                            {"value": "me-south-1", "text": "Middle East (Bahrain)"},
                            {"value": "sa-east-1", "text": "South America (SÃ£o Paulo)"},
                            {"value": "us-gov-east-1", "text": "AWS GovCloud (US-East)"},
                            {"value": "us-gov-west-1", "text": "AWS GovCloud (US-West)"}
                        ]
                    },
                    {
                        "hea": {
                            "section": "tags",
                            "sectionPrompt": "Tags"
                        },
                        "name": "key",
                        "required": true
                    },
                    {
                        "hea": {
                            "section": "tags"
                        },
                        "name": "value",
                        "required": true
                    },
                    {
                        "hea": {
                            "section": "tags",
                            "display": false
                        },
                        "name": "type",
                        "required": true,
                        "readOnly": true,
                        "value": "heaobject.root.Tag"
                    },
                    {
                        "name": "owner",
                        "prompt": "Owner",
                        "required": true,
                        "readOnly": true,
                        "type": "select",
                        "hea": {
                            "optionsFromUrl": {
                                "path": "people/",
                                "value": "id",
                                "text": "display_name"
                            }
                        }
                    },
                    {
                        "name": "created",
                        "prompt": "Created",
                        "readOnly": true,
                        "hea": {
                            "type": "datetime"
                        }
                    },
                    {
                        "name": "modified",
                        "prompt": "Modified",
                        "readOnly": true,
                        "hea": {
                            "type": "datetime"
                        }
                    },
                    {
                        "name": "source",
                        "prompt": "Source",
                        "readOnly": true
                    },
                    {
                        "hea": {
                            "cardinality": "multiple",
                            "optionsFromUrl": {
                                "path": "people/?excludesystem=yes",
                                "value": "id",
                                "text": "display_name"
                            }
                        },
                        "name": "collaborator_ids",
                        "prompt": "Collaborators",
                        "type": "select"
                    },
                    {
                        "hea": {
                            "type": "text-display"
                        },
                        "name": "collaborator_eventual_consistency",
                        "value": "Collaborator changes may take a few minutes to appear elsewhere."
                    }
                ]
            },
            {
                "name": "heaserver-buckets-bucket-new-folder-form",
                "description": "Create a new folder",
                "type": "unsafe",
                "target": "item cj-template add",
                "action": "update",
                "prompt": "New Folder",
                "inputs": [
                    {
                        "name": "display_name",
                        "prompt": "Name",
                        "required": true
                    },
                    {
                        "name": "type",
                        "prompt": "Type",
                        "required": true,
                        "readOnly": true,
                        "value": "heaobject.folder.AWSS3Folder",
                        "hea": {
                            "display": false
                        }
                    }
                ]
            },
            {
                "name": "heaserver-buckets-bucket-new-project-form",
                "description": "Create a new project",
                "type": "unsafe",
                "target": "item cj-template add",
                "action": "update",
                "prompt": "New Folder",
                "inputs": [
                    {
                        "name": "display_name",
                        "prompt": "Name",
                        "required": true
                    },
                    {
                        "name": "type",
                        "prompt": "Type",
                        "required": true,
                        "readOnly": true,
                        "value": "heaobject.project.AWSS3Project",
                        "hea": {
                            "display": false
                        }
                    }
                ]
            },
            {
                "name": "heaserver-buckets-item-get-actual",
                "description": "The actual bucket",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Get actual"
            },
            {
                "name": "heaserver-buckets-bucket-get-delete-preflight-url",
                "description": "Get the URL for getting the delete preflight form",
                "type": "safe",
                "action": "read",
                "target": "app read cj",
                "prompt": "Get the delete preflight form URL"
            },
            {
                "name": "heaserver-buckets-bucket-delete-get-preflight-form",
                "description": "Sends the selected desktop object URLs to the server for a preflight check before deleting the buckets",
                "type": "safe",
                "action": "read",
                "target": "item cj-template",
                "prompt": "Delete",
                "rel": ["hea-auto-submission", "hea-returns-data"],
                "inputs": [
                    {
                        "name": "display_name",
                        "hea": {
                            "display": false
                        }
                    },
                    {
                        "name": "desktop_object_urls",
                        "prompt": "Selected object URLs",
                        "required": true,
                        "hea": {
                            "type": "selected-object-url",
                            "cardinality": "multiple",
                            "display": false
                        }
                    }
                ]
            },
            {
                "name": "heaserver-buckets-bucket-open-as-zip",
                "description": "Open this bucket as a zip archive",
                "type": "safe",
                "action": "read",
                "target": "app read cj",
                "prompt": "Open"
            }
        ]
    }
}
