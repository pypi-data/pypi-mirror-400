{
  "_id": "oven-controller",
  "classname": "herosdevices.hardware.srs.PTC10",
  "active": true,
  "arguments": {
    "address": "192.168.50.15",
    "channels_tc": [
      "WindowTC",
      "OvenTC"
    ],
    "channels_out": [
      "WindowOut",
      "OvenOut"
    ],
    "macros": {
      "OvenOn": "if (OvenOut.PID.setpoint==0){ OvenOut.PID.setpoint=50 }else{ popup \"Oven already on\" }"
    }
  },
  "datasource": {
    "async": false,
    "interval": 300
  }
}

