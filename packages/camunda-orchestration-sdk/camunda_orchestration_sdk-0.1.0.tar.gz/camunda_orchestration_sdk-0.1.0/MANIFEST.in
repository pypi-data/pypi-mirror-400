# Include the Generated SDK
graft generated

# Exclude generation machinery and source maps
exclude generate.py
exclude bundle.py
exclude Makefile
exclude generator-config.yaml
exclude generator-config-python-client.yaml
exclude openapitools.json
prune hooks
prune hooks_shared
prune hooks_v1
prune hooks_v2
prune runtime
prune docker
prune demo
prune tests

# Utility exclusions
global-exclude *.pyc
global-exclude __pycache__
global-exclude *.DS_Store
