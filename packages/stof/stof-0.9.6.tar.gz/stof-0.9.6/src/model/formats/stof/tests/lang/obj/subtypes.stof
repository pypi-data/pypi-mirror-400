#[type]
SubType: {
    str one: 'one'
    str two: 'two'
}

#[type]
SuperType: {
    SubType sub: new SubType {}
    str msg: ''
}


#[test]
fn sub_type_casts() {
    const o = new SuperType {
        msg: 'hi'
        sub: new { one: 'ONE' }
    };
    assert_eq(typename o, 'SuperType');
    assert_eq(o.msg, 'hi');
    assert_eq(typename o.sub, 'SubType');
    assert_eq(o.sub.one, 'ONE');
    assert_eq(o.sub.two, 'two');
}
