{
  "name": "funcnodes-react-flow-monorepo",
  "version": "2.2.1a0",
  "private": true,
  "packageManager": "yarn@4.9.2",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -A run build",
    "typecheck": "yarn workspaces foreach -A run typecheck",
    "test": "yarn workspaces foreach -A run test",
    "test:coverage": "yarn workspace @linkdlab/funcnodes_react_flow test --coverage --coverage.reporter=json --coverage.reporter=json-summary --coverage.reporter=text",
    "watch": "yarn workspace @linkdlab/funcnodes_react_flow run watch",
    "dev-full": "node dev_full.cjs"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.16",
    "typescript": "^5.9.3",
    "vitest": "4.0.16"
  }
}
