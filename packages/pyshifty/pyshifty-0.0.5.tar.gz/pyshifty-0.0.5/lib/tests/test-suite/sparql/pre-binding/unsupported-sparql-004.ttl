@prefix ex: <http://datashapes.org/sh/tests/sparql/pre-binding/unsupported-sparql-004.test#> .
@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix sht: <http://www.w3.org/ns/shacl-test#> .

ex:TestShape a sh:NodeShape ;
    sh:sparql [ sh:select """
			SELECT $this
			WHERE {
				$this ?x ?any .
				{
					SELECT ?other ?b 
					WHERE {
						?other ?b ?c .
					}
				}
			}""" ] ;
    sh:targetNode ex:InvalidResource .

<urn:unsupported-sparql-004> a sht:Validate ;
    rdfs:label "Test of unsupported SELECT" ;
    mf:action [ sht:dataGraph <urn:unsupported-sparql-004.ttl> ;
            sht:shapesGraph <urn:unsupported-sparql-004.ttl> ] ;
    mf:result sht:Failure ;
    mf:status sht:approved .

<urn:unsupported-sparql-004.ttl> a mf:Manifest ;
    mf:entries ( <urn:unsupported-sparql-004> ) .

