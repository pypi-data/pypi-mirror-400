{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/packages/cspell-types/cspell.schema.json",
  "version": "0.2",
  "name": "SpreadsheetDL Spell Checker Configuration",
  "description": "cSpell configuration for Markdown-only spellchecking",

  "enabledFileTypes": {
    "markdown": true,

    "python": false,
    "javascript": false,
    "typescript": false,
    "html": false,
    "css": false,
    "scss": false,
    "less": false,
    "go": false,
    "rust": false,
    "java": false,
    "c": false,
    "cpp": false,
    "csharp": false,
    "ruby": false,
    "php": false,
    "perl": false,
    "lua": false,
    "tcl": false,
    "vhdl": false,
    "verilog": false,
    "systemverilog": false,
    "shellscript": false,
    "dockerfile": false,
    "makefile": false,
    "r": false,
    "sql": false,
    "graphql": false,

    "xml": false,
    "xsl": false,
    "svg": false,
    "latex": false,
    "bibtex": false,
    "jinja": false,
    "jinja-md": false,
    "plantuml": false,

    "json": false,
    "jsonc": false,
    "yaml": false,
    "toml": false,
    "ini": false,
    "properties": false,
    "dotenv": false,
    "csv": false,

    "plaintext": false,
    "text": false,
    "ignore": false,
    "editorconfig": false,
    "git-commit": false,
    "git-rebase": false,
    "diff": false,
    "log": false
  },

  "enabledLanguageIds": ["markdown"],

  "ignorePaths": [
    "**/*.py",
    "**/*.pyi",
    "**/*.sh",
    "**/*.bash",
    "**/*.zsh",
    "**/*.fish",
    "**/*.lua",
    "**/*.tcl",
    "**/*.pl",
    "**/*.pm",
    "**/*.tex",
    "**/*.bib",
    "**/*.cls",
    "**/*.sty",
    "**/*.js",
    "**/*.ts",
    "**/*.jsx",
    "**/*.tsx",
    "**/*.mjs",
    "**/*.cjs",
    "**/*.go",
    "**/*.rs",
    "**/*.java",
    "**/*.c",
    "**/*.h",
    "**/*.cpp",
    "**/*.hpp",
    "**/*.cs",
    "**/*.rb",
    "**/*.php",
    "**/*.r",
    "**/*.R",
    "**/*.sql",

    "**/*.vhd",
    "**/*.vhdl",
    "**/*.v",
    "**/*.sv",
    "**/*.svh",

    "**/*.html",
    "**/*.htm",
    "**/*.css",
    "**/*.scss",
    "**/*.less",
    "**/*.sass",

    "**/*.j2",
    "**/*.jinja",
    "**/*.jinja2",
    "**/*.md.j2",
    "**/*.puml",
    "**/*.plantuml",

    "**/*.yaml",
    "**/*.yml",
    "**/*.json",
    "**/*.jsonc",
    "**/*.toml",
    "**/*.xml",
    "**/*.xsl",
    "**/*.xslt",
    "**/*.xsd",
    "**/*.svg",
    "**/*.csv",
    "**/*.tsv",

    "**/.editorconfig",
    "**/.gitignore",
    "**/.gitattributes",
    "**/.gitmodules",
    "**/.dockerignore",
    "**/.npmignore",
    "**/.prettierignore",
    "**/.claudeignore",
    "**/.lycheeignore",
    "**/.shellcheckrc",
    "**/.bashrc",
    "**/.bash_profile",
    "**/.zshrc",
    "**/.profile",
    "**/.inputrc",
    "**/.perltidyrc",
    "**/.perlcriticrc",
    "**/.chktexrc",
    "**/.latexmkrc",
    "**/.luarc.json",
    "**/.luacheckrc",
    "**/pyproject.toml",
    "**/setup.py",
    "**/setup.cfg",
    "**/requirements*.txt",
    "**/Pipfile",
    "**/Pipfile.lock",
    "**/poetry.lock",
    "**/uv.lock",
    "**/package.json",
    "**/package-lock.json",
    "**/yarn.lock",
    "**/pnpm-lock.yaml",
    "**/composer.json",
    "**/composer.lock",
    "**/Cargo.toml",
    "**/Cargo.lock",
    "**/go.mod",
    "**/go.sum",
    "**/Gemfile",
    "**/Gemfile.lock",

    "**/Makefile",
    "**/makefile",
    "**/GNUmakefile",
    "**/justfile",
    "**/Justfile",
    "**/.justfile",

    "**/Dockerfile",
    "**/Dockerfile.*",
    "**/*.dockerfile",
    "**/docker-compose*.yml",
    "**/docker-compose*.yaml",

    "**/.vscode/**",
    "**/.idea/**",
    "**/.vs/**",
    "**/.eclipse/**",

    "**/.venv/**",
    "**/.env/**",
    "**/venv/**",
    "**/env/**",
    "**/.git/**",
    "**/__pycache__/**",
    "**/.mypy_cache/**",
    "**/.ruff_cache/**",
    "**/.pytest_cache/**",
    "**/.tox/**",
    "**/.nox/**",
    "**/.coverage/**",
    "**/htmlcov/**",
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/build/**",
    "**/out/**",
    "**/target/**",
    "**/.next/**",
    "**/.nuxt/**",

    "**/*.lock",
    "**/*-lock.json",
    "**/*-lock.yaml",

    "**/*.min.js",
    "**/*.min.css",
    "**/*.map",
    "**/*.d.ts",

    "**/.cspell/**"
  ],

  "language": "en",

  "dictionaries": [
    "en_US",
    "en-gb",
    "companies",
    "softwareTerms",
    "misc",
    "filetypes",
    "workspace-custom"
  ],

  "dictionaryDefinitions": [
    {
      "name": "workspace-custom",
      "path": ".cspell/custom-dictionary.txt",
      "addWords": true,
      "scope": "workspace"
    }
  ],

  "words": [],

  "ignoreRegExpList": [
    "https?://[^\\s]+",
    "ftp://[^\\s]+",
    "/[\\w/.-]+",

    "[\\w.+-]+@[\\w.-]+\\.[a-z]{2,}",

    "`[^`]+`",

    "\\.[a-z]{2,4}\\b",

    "\\$\\{?[A-Z_][A-Z0-9_]*\\}?",

    "\\b[a-f0-9]{7,40}\\b",

    "\\bv?\\d+\\.\\d+(\\.\\d+)?(-[a-z0-9]+)?\\b",

    "--?[a-z][a-z0-9-]*",

    "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
  ],

  "caseSensitive": false,

  "minWordLength": 4,

  "maxNumberOfProblems": 100,

  "maxDuplicateProblems": 5,

  "allowCompoundWords": true,

  "flagWords": [
    "hte",
    "teh",
    "adn",
    "taht",
    "accidently",
    "occured",
    "seperate",
    "definately",
    "occurance"
  ],

  "suggestionsTimeout": 500,
  "suggestionNumChanges": 3,
  "numSuggestions": 8,

  "cache": {
    "useCache": true,
    "cacheLocation": ".cspell/.cache",
    "cacheStrategy": "content"
  },

  "features": {
    "weighted-suggestions": true
  }
}
