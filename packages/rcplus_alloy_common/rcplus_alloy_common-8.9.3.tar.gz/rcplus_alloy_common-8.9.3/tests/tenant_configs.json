                    "conditions":
                    [
                        {
                            "condition":
                            {
                                "operator": "in",
                                "value":
                                [
                                    "alle-immobilien.ch"
                                ],
                                "attribute": "app_id"
                            },
                            "result":
                            {
                                "type": "regex",
                                "pattern": "(?:affitta|mieten|rent|louer|acheter|kaufen|buy|compra)/(?!(?:kanton|cantone|cantons|canton)-)([^/]+)/",
                                "string": "page_url"
                            }
                        },
                        {
                            "condition":
                            {
                                "operator": "in",
                                "value":
                                [
                                    "homegate.ch",
                                    "immoscout24.ch"
                                ],
                                "attribute": "app_id"
                            },
                            "result":
                            {
                                "type": "jsonpath",
