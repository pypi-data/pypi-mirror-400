{
  "name": "cognitive-core",
  "version": "0.0.2",
  "description": "TypeScript client for Cognitive Core - A meta-learning framework",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cognitive",
    "meta-learning",
    "ai",
    "agents",
    "trajectories",
    "arc-agi"
  ],
  "author": "Alex Ngai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexngai/meta-learning-engine",
    "directory": "ts"
  },
  "bugs": {
    "url": "https://github.com/alexngai/meta-learning-engine/issues"
  },
  "homepage": "https://github.com/alexngai/meta-learning-engine#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
