#[test]
fn len() {
    let a = (32, 43, true, 'hi');
    assert_eq(a.len(), 4);
}

#[test]
fn at() {
    let a = ('a', ('b', 42));
    assert_eq(a[1][1], 42);
}

#[test]
fn at_ref() {
    let a = ('a', ('b', 42));
    let inner = &a[1][1];
    inner = 53;
    assert_eq(a, ('a', ('b', 53)));
}
