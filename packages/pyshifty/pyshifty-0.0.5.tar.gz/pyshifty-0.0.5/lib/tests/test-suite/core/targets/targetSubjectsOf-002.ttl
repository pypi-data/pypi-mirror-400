@prefix ex: <http://datashapes.org/sh/tests/core/targets/targetSubjectsOf-002.test#> .
@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix sht: <http://www.w3.org/ns/shacl-test#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:MyClass a rdfs:Class .

ex:TestShape a sh:NodeShape ;
    sh:property ex:TestShape-myProperty ;
    sh:targetClass ex:MyClass ;
    sh:targetSubjectsOf ex:myProperty .

ex:ValidInstance1 a ex:MyClass ;
    ex:myProperty "A" .

ex:ValidInstance2 ex:myProperty "A" .

ex:InvalidInstance1 a ex:MyClass ;
    ex:myProperty "A",
        "B" .

ex:InvalidInstance2 ex:myProperty "A",
        "B" .

<urn:targetSubjectsOf-002> a sht:Validate ;
    rdfs:label "Test of sh:targetSubjectsOf 002" ;
    mf:action [ sht:dataGraph <urn:targetSubjectsOf-002.ttl> ;
            sht:shapesGraph <urn:targetSubjectsOf-002.ttl> ] ;
    mf:result [ a sh:ValidationReport ;
            sh:conforms false ;
            sh:result [ a sh:ValidationResult ;
                    sh:focusNode ex:InvalidInstance1 ;
                    sh:resultPath ex:myProperty ;
                    sh:resultSeverity sh:Violation ;
                    sh:sourceConstraintComponent sh:MaxCountConstraintComponent ;
                    sh:sourceShape ex:TestShape-myProperty ],
                [ a sh:ValidationResult ;
                    sh:focusNode ex:InvalidInstance2 ;
                    sh:resultPath ex:myProperty ;
                    sh:resultSeverity sh:Violation ;
                    sh:sourceConstraintComponent sh:MaxCountConstraintComponent ;
                    sh:sourceShape ex:TestShape-myProperty ] ] ;
    mf:status sht:approved .

<urn:targetSubjectsOf-002.ttl> a mf:Manifest ;
    mf:entries ( <urn:targetSubjectsOf-002> ) .

ex:TestShape-myProperty sh:maxCount 1 ;
    sh:path ex:myProperty .

