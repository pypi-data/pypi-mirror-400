{
  "title": "Run",
  "type": "object",
  "additionalProperties": false,
  "required": ["run_id", "state", "root_node_run_id"],
  "properties": {
    "run_id": { "type": "string" },
    "state": { "$ref": "run_state.schema.json" },
    "root_node_run_id": { "type": "string" },
    "run_context": { "$ref": "run_context.schema.json" },
    "started_at": { "type": "string", "format": "date-time" },
    "ended_at": { "type": "string", "format": "date-time" },
    "extensions": { "$ref": "../common/extensions.schema.json" }
  }
}
