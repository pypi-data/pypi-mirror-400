            },
            {
                "name": "item_condition",
                "display_name": "Item Condition",
                "category": "Product Features",
                "active": true,
                "type": "content",
                "value_pii": false,
                "query": {
                    "result": {
                        "type": "jsonpath",
                        "jsonpath": "$.offers[0].itemCondition",
                        "string": "content.schemaOrgData"
                    }
                },
                "map_values": [
                    {
                        "values": ["new", "newCondition"],
                        "name": "new"
                    },
                    {
                        "values": ["used", "usedCondition"],
                        "name": "used"
                    },
                    {
                        "values": ["refurbished", "refurbishedCondition"],
                        "name": "refurbished"
                    }
                ]
            },
            {
                "name": "rooms",
                "display_name": "Rooms",
                "category": "Real Estate",
                "active": true,
                "type": "tracking",
                "buckets": [0, 1, 2, 3],
                "query": {
                    "conditions": [
                        {
                            "condition": {
                                "operator": "in",
                                "value": ["homegatech"],
                                "attribute": "split_part(logical_path, '/', 1)"
                            },
                            "result": {
                                "type": "jsonpath",
                                "jsonpath": "$.rooms",
                                "string": "custom_tracking_attributes"
                            }
                        },
                        {
                            "condition": {
                                "operator": "in",
                                "value": ["immoscout24"],
                                "attribute": "split_part(logical_path, '/', 1)"
                            },
                            "result": {
                                "type": "jsonpath",
                                "jsonpath": "$[0].n_rooms",
                                "string": "custom_tracking_attributes"
                            }
                        }
                    ],
                    "result": {
                        "type": "jsonpath",
                        "jsonpath": "$.src_rooms",
                        "string": "custom_tracking_attributes"
                    }
                }
            }
        ],
        "kropka_tenants": ["kropka_1", "kropka_2"],
        "content_providers": [
            {
                "name": "content-provider-1",
                "section_prefixes": ["prefix_1", "prefix_2"],
                "uri_prefixes": ["www.content-provider-1.ext"],
                "source_system": "source_1",
                "content_id_extraction_query": "query_1",
                "hardcoded_taxonomies": [
                    {
                        "name": "taxonomy_1",
                        "score": 0.5
                    }
                ],
                "is_uri_extracted_id_external_id": true,
                "app_ids": ["app_1"]
            },
            {
                "name": "content-provider-2",
                "section_prefixes": ["prefix_3", "prefix_4"],
                "uri_prefixes": ["www.content-provider-2.ext"],
                "source_system": "source_2",
                "is_uri_extracted_id_external_id": false,
                "canonical_id_extraction": {
                    "uri_provider": "source_2~content-provider-2",
                    "id_regex": "(\\w+)$"
                }
            }
        ],
        "tracking_providers": [
            {
                "tracker": "tracker_0",
                "logical_paths": ["path_1"]
            },
            {
                "tracker": "tracker_1",
                "logical_paths": ["path_1", "path_2"],
                "configs": [
                    {
                        "app_ids": ["app_id_11", "app_id_12"],
                        "id_regex": "-(\\d+)$",
                        "data_layer_auto_tracking_events": [
                            {
                                "key_name": "event1",
                                "key_value": "virtual_pageview1",
                                "event_path": "data1"
                            },
                            {
                                "key_name": "event2",
                                "key_value": "virtual_pageview2"
                            }
