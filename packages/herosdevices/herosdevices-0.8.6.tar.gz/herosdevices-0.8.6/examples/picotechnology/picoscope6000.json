{
  "_id": "myscope",
  "classname": "herosdevices.hardware.picotechnology.Picoscope6000a",
  "arguments": {
    "serial_num": "JP306/0102",
    "default_config": "default",
    "config_dict": {
      "default": {
        "ch0": {
          "coupling": "PICO_DC_50OHM",
          "range": "PICO_100MV"
        },
        "ch1": {
          "coupling": "PICO_DC_50OHM",
          "range": "PICO_5V",
          "record_trace": false
        },
        "trigger": {
          "source": 1,
          "threshold": 1.5
        },
        "acquisition": {
          "trace_length": 0.000006,
          "sample_time": 2e-10
        }
      }
    }
  }
}


